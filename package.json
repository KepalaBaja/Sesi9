{
    "name": "selenium-mocha-digital-skola",
    "version": "1.0.0",
    "description": "Selenium WebDriver tests using Mocha with Mochawesome reports",
    "scripts": {
        "cobain": "mocha tests --recursive --timeout 60000",
        "run-mochawsome": "mocha tests --recursive --timeout 60000 --reporter mochawesome",
        "run-simple-html": "mocha tests --recursive --timeout 60000 --reporter mocha-simple-html-reporter --reporter-options output=HasilSesi9.html"
    },
    "dependencies": {
        "selenium-webdriver": "^4.17.0"
    },
    "devDependencies": {
        "mocha": "^10.4.0",
        "mocha-simple-html-reporter": "^2.0.0",
        "mochawesome": "^7.1.3"
    }
}